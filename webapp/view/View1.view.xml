<mvc:View controllerName="project7.controller.View1" xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
	xmlns:m="sap.m"
	xmlns="sap.uxap" xmlns:forms="sap.ui.layout.form" xmlns:layout="sap.ui.layout"
	xmlns:core="sap.ui.core" xmlns:table="sap.ui.table"
	xmlns:f="sap.f" xmlns:card="sap.f.cards" xmlns:w="sap.ui.integration.widgets">
	<ObjectPageLayout id="_IDGenObjectPageLayout1" subSectionLayout="TitleOnLeft"
		showTitleInHeaderContent="true" upperCaseAnchorBar="false" showFooter="true">
		<headerTitle>
			<ObjectPageDynamicHeaderTitle id="_IDGenObjectPageDynamicHeaderTitle1">
				<expandedHeading>
					<m:HBox id="_IDGenHBox1">
						<m:Title id="_IDGenTitle1" text="Ruben Smith" wrapping="true" />
						<m:ObjectMarker id="_IDGenObjectMarker1" type="Flagged"
							class="sapUiTinyMarginBegin" />
					</m:HBox>
				</expandedHeading>
				<snappedHeading>
					<m:FlexBox id="_IDGenFlexBox1" fitContainer="true" alignItems="Center">
						<m:Avatar id="_IDGenAvatar1" src="" class="sapUiTinyMarginEnd" />
						<m:Title id="_IDGenTitle2" text="Ruben Smith" wrapping="true" />
					</m:FlexBox>
				</snappedHeading>
				<expandedContent>
					<m:Text id="_IDGenText1" text="MRN: 12345" />
				</expandedContent>
				<snappedContent>
					<m:Text id="_IDGenText2" text="MRN: 12345" />
				</snappedContent>
				<snappedTitleOnMobile>
					<m:Title id="_IDGenTitle3" text="MRN: 12345" />
				</snappedTitleOnMobile>
				<actions>
					<m:Button id="_IDGenButton1" text="Save" type="Emphasized" press="onPressSave"
						ariaDescribedBy="defaultButtonDescription genericButtonDescription"
						icon="sap-icon://business-card">
					</m:Button>
				</actions>
			</ObjectPageDynamicHeaderTitle>
		</headerTitle>
		<headerContent>
			<m:FlexBox id="_IDGenFlexBox2" wrap="Wrap">
				<m:Avatar id="_IDGenAvatar2" class="sapUiSmallMarginEnd" initials="DS"
					displaySize="L" />
				<layout:VerticalLayout id="_IDGenVerticalLayout1" class="sapUiSmallMarginBeginEnd">
					<m:Text id="_IDGenText3" class="sapUiTinyMarginTop" text="Male" />
					<m:Text id="_IDGenText4" class="sapUiTinyMarginTop" text="DOB: 11.08.1978" />
					<m:Text id="_IDGenText5" class="sapUiTinyMarginTop" text="46yrs 23 days" />
					<m:Link id="_IDGenLink1" class="sapUiTinyMarginTop" text="+971-501234567" />
					<m:Link id="_IDGenLink2" class="sapUiTinyMarginTop" text="Denise.Smith@test.com" />
				</layout:VerticalLayout>
				<layout:HorizontalLayout id="_IDGenHorizontalLayout1"
					class="sapUiSmallMarginBeginEnd">
					<m:Image id="_IDGenImage1" src="./test-resources/sap/uxap/images/linkedin.png" />
					<m:Image id="_IDGenImage2" src="./test-resources/sap/uxap/images/Twitter.png"
						class="sapUiSmallMarginBegin" />
				</layout:HorizontalLayout>
				<layout:VerticalLayout id="_IDGenVerticalLayout2" class="sapUiSmallMarginBeginEnd">
					<m:Link id="_IDGenLink3" text="Dentist" />
					<m:Text id="_IDGenText6" class="sapUiTinyMarginTop" text="General Dentist" />
					<m:Label id="_IDGenLabel1" class="sapUiTinyMarginTop"
						text="Visit Date: 20.04.2024" />
				</layout:VerticalLayout>
				<layout:VerticalLayout id="_IDGenVerticalLayout3" class="sapUiSmallMarginBeginEnd">
					<m:ObjectStatus id="_IDGenObjectStatus1" text="Credit Limit: 1850.00"
						state="Success" />
					<m:ObjectStatus id="_IDGenObjectStatus2" class="sapUiTinyMarginTop"
						text="Overdue: 0.00" state="Error" />
				</layout:VerticalLayout>
			</m:FlexBox>
		</headerContent>
		<sections>
			<ObjectPageSection id="_IDGenObjectPageSection1" titleUppercase="false"
				title="{i18n>dignoisis}">
				<subSections>
					<ObjectPageSubSection id="_IDGenObjectPageSubSection1">
						<blocks>
							<!-- <m:VBox id="_IDGenVBox1" items="{LimitsTemplateModel>/panelData}"> -->
							<m:VBox id="_IDGenVBox1"
								items="{LimitsTemplateModel1>/to_Encounter/results}">
								<m:Panel id="_IDGenPanel1" headerText="{LimitsTemplateModel1>EncID}"
									expanded="true" expandable="true">
									<m:Table id="_IDGenTable1"
										items="{LimitsTemplateModel1>to_Diagnosis/results}">
										<m:columns>
											<m:Column id="_IDGenColumn1" minScreenWidth="Tablet"
												hAlign="Center" width="6rem"
												demandPopin="true">
												<m:Label id="_IDGenLabel2" design="Bold"
													text="Catalog" />
											</m:Column>
											<m:Column id="_IDGenColumn2" minScreenWidth="Tablet"
												hAlign="Center" width="8rem"
												demandPopin="true">
												<m:Label id="_IDGenLabel3" design="Bold" text="Code" />
											</m:Column>
											<m:Column id="_IDGenColumn3" minScreenWidth="Tablet"
												width="10rem"
												demandPopin="true">
												<m:Label id="_IDGenLabel4" design="Bold"
													text="Description" />
											</m:Column>
											<m:Column id="_IDGenColumn4" minScreenWidth="Tablet"
												demandPopin="true">
												<m:Label id="_IDGenLabel5" design="Bold"
													text="Level" />
											</m:Column>
											<m:Column id="_IDGenColumn5" minScreenWidth="Tablet"
												hAlign="Left" width="6rem"
												demandPopin="true">
												<m:Label id="_IDGenLabel6" design="Bold"
													text="Laterality" />
											</m:Column>
											<m:Column id="_IDGenColumn6" minScreenWidth="Tablet"
												hAlign="Center" width="6rem"
												demandPopin="true">
												<m:Label id="_IDGenLabel7" design="Bold"
													text="Certainty" />
											</m:Column>
											<!-- <m:Column id="_IDGenColumn7"
											minScreenWidth="Tablet"
				demandPopin="true" hAlign="Center">
					 <m:Label id="_IDGenLabel8" design="Bold" text="C1"/>
                     </m:Column>
					  <m:Column id="_IDGenColumn8" minScreenWidth="Tablet" hAlign="Center"
				demandPopin="true">
					 <m:Label id="_IDGenLabel9" design="Bold" text="C2"/>
                     </m:Column> -->
										</m:columns>
										<m:items>
											<m:ColumnListItem id="_IDGenColumnListItem1">
												<m:ComboBox id="_IDGenComboBox1"
													selectedKey="{LimitsTemplateModel1>DiagCatalog}"
													enabled="{LimitsTemplateModel1>DiagSecondary}"
													items="{/DiagnosisCatalogValueHelp}">
													<core:Item key="{value_low}" text="{text}" />
												</m:ComboBox>
												<!-- <m:Input enabled="false"
												value="{LimitsTemplateModel>Code}"/> -->
												<m:Input showSuggestion="true" enabled="true"
													value="{LimitsTemplateModel1>DiagCode}"
													liveChange="changeCode"
													id="multiInput" showValueHelp="true"
													valueHelpRequest="handleValueHelp"
													suggestionItems="{/DiagnosisCodeValueHelp}">
														<m:suggestionItems>
															<core:ListItem text="{DiagCode}" additionalText="{DiagCode_Text}" />
														</m:suggestionItems>
												</m:Input>
												<m:Input id="_IDGenInput1" enabled="false"
													value="{LimitsTemplateModel1>DiagCode}" />
												<m:SegmentedButton id="_IDGenSegmentedButton1"
													selectedKey="{=${LimitsTemplateModel1>DiagSecondary} === true ? 'S' : 'P' }" selectionChange="onChangeLevel" enabled="true">
													<m:items>
														<m:SegmentedButtonItem
															id="_IDGenSegmentedButtonItem1"
															tooltip="Primary" text="P"
															key="P"/>
														<m:SegmentedButtonItem
															id="_IDGenSegmentedButtonItem2"
															tooltip="Secondary" text="S" key="S" />

													</m:items>
												</m:SegmentedButton>
												<!-- <m:Text text="{LimitsTemplateModel>Level}"/> -->
												<m:ComboBox id="_IDGenComboBox2"
													selectedKey="{LimitsTemplateModel1>DiagLat}"
													items="{/DiagnosisLateralityValueHelp}">
													<core:Item key="{DiagLat}" text="{DiagLat_Text}" />
												</m:ComboBox>
												<m:ComboBox id="_IDGenComboBox3"
													selectedKey="{LimitsTemplateModel1>DiagCert}" items="{/DiagnosisCertaintyValueHelp}">
													<core:Item key="{DiagCert}" text="{DiagCert_Text}" />
												</m:ComboBox>
								
											</m:ColumnListItem>
										</m:items>
									</m:Table>
								</m:Panel>
							</m:VBox>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection id="_IDGenObjectPageSection2" titleUppercase="false"
				title="{i18n>Chdignoisis}">
				<subSections>
					<ObjectPageSubSection id="_IDGenObjectPageSubSection2">
						<blocks>
							<m:Table id="_IDGenTable2" items="{LimitsTabableModel>/}">
								<m:columns>
									<m:Column id="_IDGenColumn9" minScreenWidth="Tablet"
										hAlign="Center" width="6rem"
										demandPopin="true">
										<m:Label id="_IDGenLabel10" design="Bold" text="Catalog" />
									</m:Column>
									<m:Column id="_IDGenColumn10" minScreenWidth="Tablet"
										hAlign="Center" width="8rem"
										demandPopin="true">
										<m:Label id="_IDGenLabel11" design="Bold" text="Code" />
									</m:Column>
									<m:Column id="_IDGenColumn11" minScreenWidth="Tablet"
										width="10rem"
										demandPopin="true">
										<m:Label id="_IDGenLabel12" design="Bold" text="Description" />
									</m:Column>
									<m:Column id="_IDGenColumn12" minScreenWidth="Tablet"
										demandPopin="true">
										<m:Label id="_IDGenLabel13" design="Bold" text="Level" />
									</m:Column>
									<m:Column id="_IDGenColumn13" minScreenWidth="Tablet"
										hAlign="Left" width="6rem"
										demandPopin="true">
										<m:Label id="_IDGenLabel14" design="Bold" text="Laterality" />
									</m:Column>
									<m:Column id="_IDGenColumn14" minScreenWidth="Tablet"
										hAlign="Center" width="6rem"
										demandPopin="true">
										<m:Label id="_IDGenLabel15" design="Bold" text="Certainty" />
									</m:Column>
								</m:columns>
								<m:items>
									<m:ColumnListItem id="_IDGenColumnListItem2">
										<m:ComboBox id="_IDGenComboBox4"
											selectedKey="{LimitsTabableModel>DiagCatalog}"
											enabled="{LimitsTabableModel>enabledcode}"
											items="{/DiagnosisCatalogValueHelp}">
											<core:Item key="{value_low}" text="{text}" />					
										</m:ComboBox>
										<!-- <m:Input enabled="false"
										value="{LimitsTemplateModel>Code}"/> -->
										<m:Input showSuggestion="true" enabled="true"
											value="{LimitsTabableModel>DiagCode}"
											liveChange="changeCode"
											id="new" showValueHelp="true"
											suggestionItems="{sPathHeaderItemModel>/Code}"
											valueHelpRequest="handleValueHelpNew">
											<core:Item id="_IDGenItem10"
												key="{sPathHeaderItemModel>Planid}"
												text="{sPathHeaderItemModel>Planid}" />
										</m:Input>
										<m:Input id="_IDGenInput2"
											enabled="{LimitsTabableModel>enabledcode}"
											value="{LimitsTabableModel>Description}" />
										<m:SegmentedButton id="_IDGenSegmentedButton2"
											selectedKey="S"
											enabled="{LimitsTabableModel>enabledcode}">
											<m:items>
												<m:SegmentedButtonItem
													id="_IDGenSegmentedButtonItem3"
													tooltip="Primary" text="P" key="P" />
												<m:SegmentedButtonItem
													id="_IDGenSegmentedButtonItem4"
													tooltip="Secondary" text="S" key="S" />
											</m:items>
										</m:SegmentedButton>
										<m:ComboBox id="_IDGenComboBox5"
											selectedKey="{LimitsTabableModel>DiagLat}"
											enabled="true">
										</m:ComboBox>
										<m:ComboBox id="_IDGenComboBox6"
											selectedKey="{LimitsTabableModel>DiagCert}"
											enabled="{LimitsTabableModel>enabledcode}">
										</m:ComboBox>
									</m:ColumnListItem>

								</m:items>
							</m:Table>

						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
		</sections>
	</ObjectPageLayout>
</mvc:View>