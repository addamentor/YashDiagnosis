<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="valueHelpDialog"
		title="Code"
		items="{/DiagnosisCodeValueHelp}"
		search="_handleDiagCodeVHSearch"
		confirm="_handleDiagCodeVHClose"
		cancel="_handleDiagCodeVHCancel"
		multiSelect="false">
		<StandardListItem
			title="{DiagCode} - {DiagCode_Text}"
			description="{DiagCode} {DiagCode_Text}" 
			type="Active"/>
	</SelectDialog>
</core:FragmentDefinition>