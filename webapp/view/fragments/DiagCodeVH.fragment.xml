<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="valueHelpDialog"
		title="Code"
		items="{DiagCodeVH>/DiagnosisCodeValueHelp}"
		search="_handleDiagCodeVHSearch"
		confirm="_handleDiagCodeVHClose"
		cancel="_handleDiagCodeVHCancel"
		multiSelect="false">
		<StandardListItem
			title="{DiagCodeVH>DiagCatalog_Text} - {DiagCodeVH>DiagCode}"
			description="{DiagCodeVH>DiagCode_Text}" 
			type="Active"/>
	</SelectDialog>
</core:FragmentDefinition>