<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="valueHelpDialog"
		title="Code"
		items="{DiagCodeVHModel>/}"
		search="_handleDiagCodeVHSearch"
		confirm="_handleDiagCodeVHClose"
		cancel="_handleDiagCodeVHCancel"
		multiSelect="false">
		<StandardListItem
			title="{DiagCodeVHModel>DiagCatalog_Text} - {DiagCodeVHModel>DiagCode}"
			description="{DiagCodeVHModel>DiagCode_Text}" 
			type="Active"/>
	</SelectDialog>
</core:FragmentDefinition>